<body>

    <div  class="container-fluid">
        <div id="menu">
            {{ render(controller(
        'App\\Controller\\MenuController:menu'
    )) }}
        </div>
        {% block header %}
            <div class="container formCheck bg-orange">
                <form class="form-inline" method="POST" action="{{path('search')}}">
                    <div class="form-group">
                        <label class="sr-only">URL</label>
                        <p class="form-control-static">{{'menu.checkUrl'|trans({}, 'menu')}}</p>
                    </div>
                    <div class="form-group mx-sm-3">
                        <label for="inputPassword2" class="sr-only">URL</label>
                        <input type="text" name='uri' class="form-control" id="inputUrl" placeholder="URL">
                    </div>
                    <button type="submit" class="btn btn-dark">{{'menu.button.check'|trans({}, 'menu')}}</button>
                </form>
            </div>
        {% endblock %}
    {% block body %}{% endblock %}
</div>

